#include<iostream>
#include<cstdlib>
#include<ctime>

using namespace std;

int main()
{
srand((unsigned int) time(NULL));
int number=(rand()%!00)+1;// 0-99->1-100
int guess=0;

do
{
cout<<"enterr guess(1-100):";
cin>>guess;
if(guess>number)
cout<<"Guess lower!"<<endl;
else if(guess<number)
cout<<"Guess higher"<<endl;
else
cout<<"You won!"<<endl;

} while(guess!=number);
returnÂ 0;
}
